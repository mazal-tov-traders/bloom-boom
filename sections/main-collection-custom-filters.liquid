<div class="t4s-section-inner t4s_nt_se_{{sid}} t4s_se_{{sid}} {{stt_layout}} {% if stt_image_bg != blank and stt_layout != 't4s-se-container' %} t4s-has-imgbg lazyloadt4s {% endif %}"  {% if stt_image_bg != blank and stt_layout != 't4s-se-container' %} data-bgset="{{ stt_image_bg | image_url: width: 1 }}" data-sizes="auto"  data-optimumx="2" {% endif %} {% render 'section_style',se_stts:se_stts %} >

<div class="custom-collection-content">

  {%- if stt_layout == 't4s-se-container custom-collection-content' -%}<div class="t4s-container-innert4s-active-filter {% if stt_image_bg != blank %} t4s-has-imgbg lazyloadt4s {% endif %} "  {% if stt_image_bg != blank %}  data-bgset="{{ stt_image_bg | image_url: width: 1 }}" data-sizes="auto"  data-optimumx="2" {% endif %} > {%- endif -%}
  {%- if se_stts.position_desc == '1' and se_stts.show_desc -%}<div id="t4s-desc-collection" class="t4s-desc-collection t4s-desc-before">{{collection.description}}</div>{%- endif -%} 
  
  <div class="custom-facets-wrap">
    <div class="custom-facets">
      <div class="custom-facets__head">
        <span class="custom-facets__title">Filters</span>
        <div class="custom-facets__head-btns">
          <button class="custom-facets__close t4s-drawer__close" data-drawer-close aria-label="{{ 'search.general.close_search' | t }}">
          <svg class="t4s-iconsvg-close" role="presentation" viewBox="0 0 16 14"><path d="M15 0L1 14m14 0L1 0" stroke="currentColor" fill="none" fill-rule="evenodd"></path></svg>
        </button>
        <button id="submit-filter-btn" type="button">Go</button>
        </div>
      </div>
 
      {%- capture filters__active -%}
        {%- for filter in collection.filters -%}
          {%- for filter_value in filter.active_values -%}
            {%- assign total_active_values = total_active_values | plus: filter.active_values.size -%}
          {%- endfor -%}
        {%- endfor -%}
        {%- if total_active_values > 1 %}<a href="{{ collection.url }}{% if collection.sort_by != blank %}?sort_by={{ collection.sort_by }}{% endif %}" class="custom-filters__clear">Remove Filter</a>{% endif -%}
      {%- endcapture -%}
      {%- if total_active_values > 0 %}<div class="custom t4s-active-filters">{{- filters__active -}}</div>{% endif -%}

      <div class="custom-facets__filters">
      </div>
    </div>
  </div> 

  <script>
    // const resultsHandleTags = [...new Set('{{results.handle}}'.replace(/\s/g,'').split('-'))].sort();
    const active_values = [];

    {% for filter in results.filters %}
      {% for active_value in filter.active_values %}
        active_values.push('{{ active_value.param_name }}' + '{{ active_value.label }}')
      {% endfor  %}
    {% endfor %}

    fetch('https://51034d.myshopify.com/api/2023-07/graphql.json', {
      method: 'POST',
      headers: {
        Accept: "application/graphql",
        "Content-Type": "application/graphql",
        "X-Shopify-Storefront-Access-Token":"c90f445bd1ca01a871de0fd6188abd33",
      },
      body: `query collection {
                collection: collection(handle: "new-filters") {
                  products: products(first: 250) {
                    filters {
                      id
                      label
                      values {
                        id
                        label
                        count
                      }
                    }
                  }
                }
              }`
    })
    .then(res => res.json())
    .then(obj => {
      const filtersContainer = document.querySelector('.custom-facets__filters');
      if (obj.data?.collection?.products?.filters) {
        obj.data.collection.products.filters.forEach((filter, index) => {

          if (filter.id.includes('filter.p.m.custom.family')) {
            return 
          }

          const filterElem = document.createElement('div');
          filterElem.setAttribute('class', `t4s-col-item t4s-col-12 t4s-facet custom-facets-filter`);
          filterElem.setAttribute('id', `blockid_${index}`);
          filterElem.innerHTML = `<h5 class="t4s-facet-title custom-facets-filter__title">${ filter.label }</h5>`;
          const filterContentElem = document.createElement('div');
          filterContentElem.setAttribute('class', 't4s-facet-content');
          const ulElem = document.createElement('ul');
          ulElem.setAttribute('class', 'custom-facets-filter__list t4s-filter__values is--style-checkbox t4s-current-scrollbar');

          const lstEl = document.createElement('div');

          filter.values.forEach(value => {
            const valArray = value.id.split('.');
            const searchVal = valArray.pop();
            const paramName = valArray.join('.');
            const valueElem = `<li class="custom-facets-filter__list-item">
                                <label>
                                  <input data-param-name="${ filter.id }" data-value="${ searchVal }" data-label="${ value.label }" type="checkbox" ${active_values.includes(paramName + value.label) | '{{results.handle}}'.includes(searchVal) && 'checked'}>
                                    <span>${ value.label } </span><span class="t4s-value-count">(${ value.count })</span>
                                </label>
                              </li>`

            
            if (['up to 150$', 'Mixed bouquets', 'Mono bouquets'].includes(value.label)) {
              const spEl = document.createElement('div');
              spEl.innerHTML = valueElem.trim();
              ulElem.prepend(spEl.firstChild);
            } else if (value.label === 'over $900') {
              lstEl.innerHTML = valueElem.trim();
            } else {
              ulElem.innerHTML += valueElem.trim(); 
            }

            if (lstEl.innerHTML) {
              ulElem.append(lstEl.firstChild);
            }
          })

          filterContentElem.appendChild(ulElem);
          filterElem.appendChild(filterContentElem);
          filtersContainer.appendChild(filterElem);
        })
      }
    })
    .catch(err => console.error(err))

      const submitFilterButton = document.querySelector('#submit-filter-btn');

      submitFilterButton.addEventListener('click', async e => {
        const tags = [];
        const params = [];
        Array.from(document.querySelectorAll('.custom-facets-filter__list-item > * > input:checked')).forEach(obj => {
          tags.push(obj.dataset.value);
          params.push({
            'name': obj.dataset.paramName,
            'value': obj.dataset.label
          });
        })
        
        if (tags.length) {

            fetch('https://51034d.myshopify.com/api/2023-07/graphql.json', {
              method: 'POST',
              headers: {
                Accept: "application/graphql",
                "Content-Type": "application/graphql",
                "X-Shopify-Storefront-Access-Token":"c90f445bd1ca01a871de0fd6188abd33",
                },
              body: `{
                collections(first: 10, query: "${tags.map(obj => 'title:' + obj).join(' AND ')}*") {
                  nodes {
                    id
                    handle
                  }
                }
              }`
            })
            .then(res => res.json())
            .then(obj => {
              
              if (obj.data.collections.nodes.length) {

                const sortedNodes = obj.data.collections.nodes.sort((a, b)=>{
                    return a.handle.length - b.handle.length
                })

                const sortedTags = [...new Set(tags.map(obj => obj.split('-')).flat())].sort();
                const sortedNodeTags = [...new Set(sortedNodes[0].handle.replace(/\s/g,'').split('-'))].sort();

                if (sortedTags.toString() === sortedNodeTags.toString()) {
                  const url = '/collections/' + sortedNodes[0].handle;
                  return window.location.replace(url)
                }

              }
            })
            .catch(err => console.error(err))
        }

        if (params.length) {
          const url = new URL(window.location.origin + window.location.pathname)
          params.forEach(param => {
            url.searchParams.append(param.name, param.value);
          })

          return window.location.replace(url)
        }

      }); 
  </script>
 
