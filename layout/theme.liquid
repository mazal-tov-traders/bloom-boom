      {{ content_for_header }}
      {%- render 'head_assets', t_name: t_name, isRTL: isRTL -%}


      {% render 'shogun-head' %}
      <script src="https://magic-menu.risingsigma.com/js/create-magic-menu.min.js" async></script>
      {% render 'rapid-search-settings' %}
      {% include 'mezereon-header-script' %}
      {%- if t_name != 'list-collections'  -%}
        {% if collection.metafields.custom.meta_tag %}
          {{ collection.metafields.custom.meta_tag.value }}
        {% else %}
          <meta name="robots" content="index, follow" />
        {% endif %}
      {% endif %} 
    </head>

    <body class="{% if content_for_header contains '.mezereon' and shop.metafields.mezereon.status == 'enabled' %} mezereon {% endif %}template-{{ request.page_type | handle }} {{class_lazy}}"{% if body_img != blank %}data-bgset="{{ body_img | image_url: width: 1 }}"data-optimumx="2"data-sizes="auto"{% endif -%}>
      {% if isRTL %}
        <script type="text/javascript" id="t4s-flicker-fix">// Flicker fix.</script>
      {% endif -%}
      <a class="skip-to-content-link visually-hidden" href="#MainContent">{{ "accessibility.skip_to_text" | t }}</a>
      <div class="t4s-close-overlay t4s-op-0"></div>
  <!-- Google Tag Manager (noscript) -->
<noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-WJRKZTL"
height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
<!-- End Google Tag Manager (noscript) -->

      <div class="t4s-website-wrapper">
        {%- render 'header', t_name: t_name -%}

        <main
          id="MainContent"
          class="content-for-layout focus-none"
          role="main"
          tabindex="-1">
          {% render 'magic-menu' %}
          {{ content_for_layout }}
        </main>

        <footer id="t4s-footer">
          {%- section 'footer' -%}
          {%- section 'bottom-bar' -%}
        </footer>
      </div>

      <ul hidden class="t4s-d-none">
        <li id="a11y-refresh-page-message">{{ 'accessibility.refresh_page' | t }}</li>
      </ul>
      {%- render 'render_bottom' -%}
      {%- render "ecom_footer" -%}{% include 'mezereon-end-of-body' %}

      {% include 'swymSnippet' %}

      {% include 'ddm_calendar' %} {{ 'ddm_calendar.css' | asset_url | stylesheet_tag }}
      
    

  <style>
.product-icon-list.vstar-star {
display: none !important;
}
</style>
    
